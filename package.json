{
  "name": "planer",
  "version": "1.1.1",
  "description": "Remove reply quotations from emails",
  "main": "lib/planer.js",
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "scripts": {
    "test": "make test",
    "compile": "coffee -o lib -c src",
    "lint": "gts check",
    "clean": "./node_modules/lever-ts-dev/scripts/clean.sh",
    "compile-coffee": "find . -name '*.coffee' | perl -ne '/node_modules\\/(?!(l-|lever-))/ || /\\/test\\// || print' | xargs -n300 -P8 coffee -bc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lever/planer.git"
  },
  "keywords": [
    "email",
    "quotations",
    "reply",
    "talon"
  ],
  "author": "Leighton Wallace (https://github.com/l8on)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lever/planer/issues"
  },
  "homepage": "https://github.com/lever/planer#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "coffee-script": "^1.10.0",
    "coffeescript": "git+ssh://git@github.com/lever/coffeescript.git#coffee1-source-map-support",
    "jsdom": "^11.6.0",
    "lever-ts-dev": "^3.6.0",
    "lever-test": "^11.15.0"
  },
  "files": [
    "**/*.coffee",
    "**/*.ts",
    "**/*.d.ts",
    "**/*.js",
    "**/*.js.map",
    "!prettier.config.js",
    "!.git/**",
    "!test/**"
  ]
}
